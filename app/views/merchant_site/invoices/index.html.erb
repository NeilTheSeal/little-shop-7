<h1> Merchant: <%= @merchant.name %> Invoices </h1>

<section class="merchant_invoices">
  <ul>
    <% @invoices.each do |invoice| %>
      <div id="invoice-<%= invoice.id %>">
        <p><li> <%= link_to "Invoice ##{invoice.id}", merchant_invoice_path(@merchant, invoice), method: :get %> </li></p>
      </div>
    <% end %>
  </ul>  
</section>